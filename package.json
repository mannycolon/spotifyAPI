{
  "name": "spotifyAPI",
  "description": "React Native app that uses the Spotify Web API to look up artists",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "signed-apk": "keytool -genkey -v -keystore my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "release-apk": "cd android && ./gradlew assembleRelease",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "15.4.2",
    "react-native": "0.41.1"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "18.1.0",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
